     "data": {
      "text/latex": [
       "$\\displaystyle y{\\left(x \\right)} = C_{1} e^{- 2 x} + \\frac{x}{2} - \\frac{1}{4}$"
      ],
      "text/plain": [
       "Eq(y(x), C1*exp(-2*x) + x/2 - 1/4)"
      ]
     },
     "execution_count": 8,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "x = symbols('x')\n",
    "\n",
    "y = Function('y')\n",
    "\n",
    "dsolve(Eq(Derivative(y(x), x) + 2*y(x) - x, 0), y(x))"
   ]
  },
