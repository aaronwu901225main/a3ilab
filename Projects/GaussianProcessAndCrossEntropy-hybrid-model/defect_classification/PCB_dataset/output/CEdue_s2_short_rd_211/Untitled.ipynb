      ],
      "text/plain": [
       "Eq(y(x), C1*exp(2*x) + 4*x**2 + 4*x + 2)"
      ]
     },
     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "dsolve(Eq(y(x).diff(x) - 2*y(x) , -8*(x**2)), y(x))"
